  "main_section_html": "<!-- User Avatar (Top Right) -->\n<div id=\"user-avatar\" class=\"position-absolute top-0 end-0 m-3\"></div>\n\n<div class=\"container text-center my-5\">\n  <h1 class=\"display-5 fw-bold mb-4\">üôã‚Äç‚ôÇÔ∏è Your Profile</h1>\n  <div id=\"profile-content\" class=\"text-start mx-auto\" style=\"max-width: 600px;\">\n    <p><strong>Name:</strong> <span id=\"member-name\">Loading...</span></p>\n    <p><strong>Email:</strong> <span id=\"member-email\">Loading...</span></p>\n    <p><strong>Membership Start Date:</strong> <span id=\"start-date\">Loading...</span></p>\n    <p><strong>Membership End Date:</strong> <span id=\"end-date\">Loading...</span></p>\n    <p><strong>Days Remaining:</strong> <span id=\"days-left\">Calculating...</span></p>\n    <div id=\"trainer-info\" style=\"display:none;\">\n      <p><strong>Trainer Name:</strong> <span id=\"trainer-name\"></span></p>\n      <p><strong>Trainer Contact:</strong> <span id=\"trainer-contact\"></span></p>\n    </div>\n  </div>\n</div>\n\n<script>\nfrappe.ready(() => {\n  const email = frappe.session.user;\n\n  // Avatar\n  frappe.call(\"frappe.client.get\", {\n    doctype: \"User\",\n    name: email\n  }).then(res => {\n    const user = res.message;\n    const fullName = user.full_name || user.first_name || user.name;\n    const initials = fullName.split(\" \").map(w => w[0]).join(\"\").substring(0, 2).toUpperCase();\n    document.getElementById(\"user-avatar\").innerHTML = `\n      <div style=\"\n        width: 45px;\n        height: 45px;\n        border-radius: 50%;\n        background-color: #007bff;\n        color: white;\n        font-weight: bold;\n        font-size: 16px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        box-shadow: 0 2px 6px rgba(0,0,0,0.2);\n      \">${initials}</div>`;\n  });\n\n  // Get Gym Membership (using email_id)\n  frappe.call({\n    method: \"frappe.client.get_list\",\n    args: {\n      doctype: \"Gym Membership\",\n      filters: { email_id: email },\n      fields: [\"name\", \"name1\", \"email_id\", \"start_date\", \"end_date\"],\n      limit: 1\n    },\n    callback: function(res) {\n      const list = res.message;\n      if (!list || list.length === 0) return;\n\n      const doc = list[0];\n      document.getElementById(\"member-name\").innerText = doc.name1 || \"N/A\";\n      document.getElementById(\"member-email\").innerText = doc.email_id || \"N/A\";\n      document.getElementById(\"start-date\").innerText = doc.start_date || \"N/A\";\n      document.getElementById(\"end-date\").innerText = doc.end_date || \"N/A\";\n\n      const today = new Date();\n      const endDate = new Date(doc.end_date);\n      const daysLeft = Math.ceil((endDate - today) / (1000 * 60 * 60 * 24));\n      document.getElementById(\"days-left\").innerText = daysLeft >= 0 ? `${daysLeft} day(s)` : \"Expired\";\n    }\n  });\n\n  // Get Gym Class Booking & Trainer Info\n  frappe.call({\n    method: \"frappe.client.get_list\",\n    args: {\n      doctype: \"Gym Class Booking\",\n      filters: { email: email },\n      fields: [\"trainer_name\"],\n      limit: 1\n    },\n    callback: function(res) {\n      if (res.message && res.message.length > 0) {\n        const trainer_name = res.message[0].trainer_name;\n        if (trainer_name) {\n          frappe.call({\n            method: \"frappe.client.get\",\n            args: {\n              doctype: \"TrainerReg\",\n              name: trainer_name\n            },\n            callback: function(trainerRes) {\n              const trainerData = trainerRes.message;\n              if (trainerData) {\n                document.getElementById(\"trainer-name\").innerText = trainerData.name1 || \"N/A\";\n                document.getElementById(\"trainer-contact\").innerText = trainerData.contact_no || \"N/A\";\n                document.getElementById(\"trainer-info\").style.display = \"block\";\n              }\n            }\n          });\n        }\n      }\n    }\n  });\n});\n</script>\n",
